algoritmo "ordem inversa"
// Função :
// Autor :
// Data : 09/06/2020
// Seção de Declarações 
var
   a, b, c : inteiro
inicio
      escreval("Digite 3 números abaixo")
      leia(a)
      leia(b)
      leia(c)
      
      escreval
      
      escreval("ORDEM INVERSA:")
      escreval(c)
      escreval(b)
      escreval(a)
fimalgoritmo
